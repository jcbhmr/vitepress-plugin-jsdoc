{
  "name": "vitepress-plugin-jsdoc",
  "version": "2.0.0",
  "keywords": [
    "vitepress",
    "jsdoc",
    "documentation",
    "plugin",
    "integration",
    "annotations",
    "codebase",
    "developer tools",
    "static",
    "vue"
  ],
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:docs": "vitepress build docs",
    "dev": "vitepress dev test/docs",
    "docs:dev": "tsc && vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "test": "vitepress build test/docs",
    "lint": "tsc --noEmit",
    "build": "tsc",
    "format": "prettier -w ."
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-jsdoc"
    ],
    "proseWrap": "always"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "jsdoc": "^4.0.2",
    "jsdoc-plugin-typescript": "^2.2.1",
    "prettier": "^3.0.3",
    "prettier-plugin-jsdoc": "^1.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitepress": "^1.0.0-rc.22",
    "vue": "^3.3.6"
  },
  "dependencies": {
    "execa": "^8.0.1",
    "glob": "^10.3.10",
    "jsdoc-api": "^8.0.0",
    "jsdoc-json": "^2.0.2",
    "path-to-regexp": "^6.2.1"
  }
}
